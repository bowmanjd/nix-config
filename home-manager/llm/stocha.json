      "apiBase": "http://127.0.0.1:1173",
      "apiKey": "var.litellm_key"
    }
  },
  "defaultModel": "gpt-4.1",
  "embeddingModel": "qwen3-embedding-0.6b",
  "providers": {
    "copilot": {
      "apiBase": "https://api.githubcopilot.com",
      "apiKeyFunction": "copilot#getKey",
      "chatEndpoint": "/chat/completions",
      "embeddingEndpoint": "/embeddings",
      "embeddingOutput": "data.0.embedding",
      "embeddingInput": "input.0",
      "embeddingDimensionsInput": "dimensions",
      "additionalHeaders": {
        "Copilot-Integration-Id": "vscode-chat",
        "Editor-Version": "stocha/0.2.0"
      }
    },
    "litellm": {
      "merge_var": "var.litellm",
      "chatEndpoint": "/v1/chat/completions",
      "embeddingEndpoint": "/embeddings",
      "embeddingOutput": "data.0.embedding",
      "embeddingDimensionsInput": "dimensions"
    },
    "openrouter": {
      "apiBase": "https://openrouter.ai/api",
      "apiKey": "env.OPENROUTER_API_KEY",
      "chatEndpoint": "/v1/chat/completions"
    },
    "groq": {
      "apiBase": "https://api.groq.com/openai",
      "apiKey": "env.GROQ_API_KEY",
      "chatEndpoint": "/v1/chat/completions"
    },
    "boron": {
      "apiBase": "http://10.0.0.10:11434",
      "chatEndpoint": "/api/chat",
      "embeddingEndpoint": "/api/embed",
      "embeddingOutput": "embeddings.0",
      "optionsInput": "options",
      "responseOutput": "message.content",
      "thinkingOutput": "message.thinking",
      "responseNoStreamOutput": "message.content"
    },
    "ollama": {
      "apiBase": "http://127.0.0.1:11434",
      "chatEndpoint": "/api/chat",
      "embeddingEndpoint": "/api/embed",
      "embeddingOutput": "embeddings.0",
      "optionsInput": "options",
      "responseOutput": "message.content",
      "thinkingOutput": "message.thinking",
      "responseNoStreamOutput": "message.content"
    },
    "gemini-embedding": {
      "apiBase": "https://generativelanguage.googleapis.com/v1beta/models",
      "apiKey": "env.GEMINI_API_KEY",
      "apiKeyHeader": "x-goog-api-key",
      "embeddingEndpoint": "/gemini-embedding-001:embedContent",
      "embeddingOutput": "embedding.values",
      "embeddingInput": "content.parts.0.text",
      "embeddingDimensionsInput": "output_dimensionality"
    },
    "llama-embedding": {
      "apiBase": "http://127.0.0.1:3383",
      "embeddingEndpoint": "/embeddings",
      "embeddingOutput": "0.embedding.0"
